# API Configuration
REACT_APP_API_BASE_URL=http://localhost:8000/api

# Brand Configuration
REACT_APP_BRAND_NAME=Digital Health Passport

# Theme Configuration
REACT_APP_THEME_VARIANT=primary
REACT_APP_THEME_BACKGROUND=primary
REACT_APP_THEME_TEXT=dark
REACT_APP_PRIMARY_COLOR=#007bff
REACT_APP_SECONDARY_COLOR=#6c757d
REACT_APP_ACCENT_COLOR=#28a745

# Route Configuration
REACT_APP_ROUTE_HOME=/
REACT_APP_ROUTE_DASHBOARD=/dashboard
REACT_APP_ROUTE_LOGIN=/login
REACT_APP_ROUTE_LOGOUT=/logout

# External Links
REACT_APP_HELP_URL=https://help.digitalhealthpassport.gov.in
REACT_APP_SUPPORT_URL=https://support.digitalhealthpassport.gov.in
REACT_APP_PRIVACY_URL=/privacy
REACT_APP_TERMS_URL=/terms

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_DEBUG_MODE=false
REACT_APP_ENABLE_OFFLINE_MODE=false

# Language Configuration
REACT_APP_DEFAULT_LANGUAGE=en
REACT_APP_SUPPORTED_LANGUAGES=en,ml,hi

# Security Configuration
REACT_APP_SESSION_TIMEOUT=3600000
REACT_APP_ENABLE_TWO_FACTOR=false

# Development Configuration
REACT_APP_DEV_TOOLS_ENABLED=true
REACT_APP_MOCK_API_ENABLED=false
