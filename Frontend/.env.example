# Digital Health Passport Environment Configuration

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:8000/api
REACT_APP_API_TIMEOUT=10000

# Authentication
REACT_APP_JWT_SECRET=your-jwt-secret-key-here
REACT_APP_TOKEN_EXPIRY=24h

# Application Settings
REACT_APP_NAME=Digital Health Passport
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development

# External Services
REACT_APP_MAPS_API_KEY=your-google-maps-api-key
REACT_APP_QR_API_URL=https://api.qrserver.com/v1/create-qr-code

# File Upload Settings
REACT_APP_MAX_FILE_SIZE=5MB
REACT_APP_ALLOWED_FILE_TYPES=image/jpeg,image/png,application/pdf

# Feature Flags
REACT_APP_ENABLE_CHATBOT=true
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_NOTIFICATIONS=true

# Localization
REACT_APP_DEFAULT_LANGUAGE=en
REACT_APP_SUPPORTED_LANGUAGES=en,ml,hi

# Security
REACT_APP_ENABLE_HTTPS=false
REACT_APP_CORS_ORIGIN=http://localhost:3000

# Development
REACT_APP_DEBUG=true
REACT_APP_MOCK_DATA=true

# Production URLs (uncomment for production)
# REACT_APP_API_BASE_URL=https://api.digitalhealthkerala.gov.in/api
# REACT_APP_ENABLE_HTTPS=true
# REACT_APP_DEBUG=false
# REACT_APP_MOCK_DATA=false
